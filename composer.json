{
  "name": "bazikyan/laravel-cache-fallback",
  "description": "Allow Laravel cache connections to fallback to more stable drivers.",
  "version": "1.0.0",
  "keywords": [
    "laravel",
    "cache",
    "fallback",
    "driver"
  ],
  "homepage": "https://github.com/bazikyan/laravel-cache-fallback",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Bour",
      "email": "mathieu.tin.bour@gmail.com"
    },
    {
      "name": "Bazikyan Argishti",
      "email": "bazikyan96@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/cache": "^9.0",
    "illuminate/config": "^9.0",
    "illuminate/support": "^9.0",
    "illuminate/container": "^9.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Windy\\CacheFallback\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Windy\\CacheFallback\\Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Windy\\CacheFallback\\CacheFallbackServiceProvider"
      ]
    }
  }
}
